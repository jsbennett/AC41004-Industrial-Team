block content
	div
		ul.nav.nav-tabs
			li.nav-item
				a.nav-link(href='#' id='Test' onClick="Test();") Test
			li.nav-item
				a.nav-link(href='#' id='Soil' onClick="Soil(this);") Soil
			li.nav-item
				a.nav-link.active(href='#' id='Summary' onClick="Summary();") Summary
			li.nav-item
				a.nav-link(href='#') Link
			h2.summaryHeader farm summary
			br
			//p #{JSON.stringify(data)}
			for result in data
				p.label Crops: 
				p.result #{result.crops}
				br
				p.label Weather: 
				p.result #{result.weather}

			link(rel='stylesheet', href='/stylesheets/stylesheet.css')