block content
	div(id='moisture' data-bind=data.field.moistureLevel style='display:none')
	div
		ul.nav.nav-tabs
			li.nav-item
				a.nav-link(href='#' id='Crop' data-bind=data.field.farmFieldID onClick="Crop();") Crop
			li.nav-item
				a.nav-link.active(href='#' id='Soil' onClick="Soil();") Soil
		div(id='id' data-bind=data.field.farmFieldID style='display:none')
		.container
			div(class='phstyle')
				//- image used to display pH heading
				img(src='/images/headings/phlevel.png' class="phHeading")
				//- image used to display pH level
				img(src="/images/ph/ph"+data.field.phLevel+".png" style="height: 140px; width:140px;", class="phLevel")
			hr(style="margin-top: 20px;")
			//- image used to display soil moisture heading
			img(src='/images/headings/soilmoisture.png' class="soilMoistureHeading")
			#chart_div(style='width: 400px; height: 180px; padding-top: 20px; padding-left:55px;')
			div(style="clear:both;")
		script(src="/javascript/gauge.js")
		div(style="height:50px;")
	style
		include ../public/stylesheets/stylesheet.css
//
